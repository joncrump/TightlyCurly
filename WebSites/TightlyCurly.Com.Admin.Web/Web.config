<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appSettings>
    <add key="ingredientsUrlPrefix" value="ingredients/" />
    <add key="ingredientsUrlPriority" value="High" />
    <add key="ingredientsUrlChangeFrequency" value="2" />
    <add key="baseSiteUrl" value="http://www.tightlycurly.com/" />
    <add key="questionsUrlPrefix" value="questions/" />
    <add key="questionsUrlPriority" value="High" />
    <add key="questionsUrlChangeFrequency" value="2" />
    <add key="defaultPage" value="welcome" />
  </appSettings>
	<connectionStrings>
		<remove name="TightlyCurly" />
		<add name="TightlyCurly" connectionString="data source=(local);Integrated Security=SSPI;Initial Catalog=TIGHTLYCURLY" providerName="System.Data.SqlClient" />
		<!--<add name="TightlyCurly" connectionString="data source=sql393.mysite4now.com;User Id=4eGd_L8M7pz2JE3xo;Password=Tr6Cd57PqjQ82aNZ4i9wR;Initial Catalog=TIGHTLYCURLY" providerName="System.Data.SqlClient"/>-->
		<remove name="ALLIGOSHEE_HAIR_ADMINConnectionString" />
		<add name="ALLIGOSHEE_HAIR_ADMINConnectionString" connectionString="Data Source=(local);Initial Catalog=TIGHTLYCURLY_ADMIN;Integrated Security=True" providerName="System.Data.SqlClient" />
		<!--<add name="ALLIGOSHEE_HAIR_ADMINConnectionString" connectionString="Data Source=SQL402.mysite4now.com;Initial Catalog=TIGHTLYCURLTOOLS_42810;User Id=4eGd_L8M7pz2JE3xo_42810;Password=Tr6Cd57PqjQ82aNZ4i9wR;" providerName="System.Data.SqlClient"/>-->
	</connectionStrings>
	<system.web>
		<!-- 
            Set compilation debug="true" to insert debugging 
            symbols into the compiled page. Because this 
            affects performance, set this value to true only 
            during development.
        -->
		<compilation debug="true" targetFramework="4.0">
		</compilation>
		<!--
            The <authentication> section enables configuration 
            of the security authentication mode used by 
            ASP.NET to identify an incoming user. 
        -->
		<authentication mode="Forms">
			<forms name=".ASPXAUTH" loginUrl="Default.aspx" />
		</authentication>
		<authorization>
			<deny users="?" />
		</authorization>
		<!--
            The <customErrors> section enables configuration 
            of what to do if/when an unhandled error occurs 
            during the execution of a request. Specifically, 
            it enables developers to configure html error pages 
            to be displayed in place of a error stack trace.

        <customErrors mode="RemoteOnly" defaultRedirect="GenericErrorPage.htm">
            <error statusCode="403" redirect="NoAccess.htm" />
            <error statusCode="404" redirect="FileNotFound.htm" />
        </customErrors>
        -->
    <httpRuntime requestValidationMode="2.0" />
		<httpModules />
		<membership defaultProvider="UserMembershipProvider">
			<providers>
				<clear />
				<add name="UserMembershipProvider" type="TightlyCurly.Com.Admin.Web.Providers.UserMembershipProvider" connectionStringName="ALLIGOSHEE_HAIR_ADMINConnectionString" minRequiredPasswordLength="5" minRequiredNonalphanumericCharacters="0" requiresQuestionAndAnswer="false" applicationName="adminApplication" />
			</providers>
		</membership>
		<!--<profile>
      <providers>
        <clear/>
        <add name="UserRoleProvider" 
             connectionStringName="ALLIGOSHEE_HAIR_ADMINConnectionString" 
             applicationName="adminApplication" 
             type="TightlyCurly.Com.Admin.Web.Providers.UserRoleProvider"/>
      </providers>
    </profile>-->
		<roleManager enabled="true" cacheRolesInCookie="true" defaultProvider="UserRoleProvider" cookieName=".ASPXROLES" cookiePath="/" cookieTimeout="30" cookieRequireSSL="false" cookieSlidingExpiration="true" createPersistentCookie="false" cookieProtection="All">
			<providers>
				<clear />
				<add name="UserRoleProvider" type="TightlyCurly.Com.Admin.Web.Providers.UserRoleProvider" connectionStringName="ALLIGOSHEE_HAIR_ADMINConnectionString" applicationName="adminApplication" />
			</providers>
		</roleManager>
		<pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID" /></system.web>
	<!-- 
        The system.webServer section is required for running ASP.NET AJAX under Internet
        Information Services 7.0.  It is not necessary for previous version of IIS.
    -->
	<system.serviceModel>
		<behaviors>
			<serviceBehaviors>
				<behavior name="TightlyCurly.Com.Admin.Web.Services.EmailServiceBehavior">
					<serviceMetadata httpGetEnabled="true" />
					<serviceDebug includeExceptionDetailInFaults="false" />
				</behavior>
			</serviceBehaviors>
		</behaviors>
		<services>
			<service behaviorConfiguration="TightlyCurly.Com.Admin.Web.Services.EmailServiceBehavior" name="TightlyCurly.Com.Admin.Web.Services.EmailService">
				<endpoint address="" binding="wsHttpBinding" contract="TightlyCurly.Com.Admin.Web.Services.IEmailService">
					<identity>
						<dns value="localhost" />
					</identity>
				</endpoint>
				<endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
			</service>
		</services>
	</system.serviceModel>
    <system.webServer>
        <modules>
            <remove name="ServiceModel" />
            <add name="ErrorHandlerModule" type="System.Web.Mobile.ErrorHandlerModule, System.Web.Mobile, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" preCondition="managedHandler" />
            <add name="ServiceModel" type="System.ServiceModel.Activation.HttpModule, System.ServiceModel.Activation, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" preCondition="managedHandler" />
            <add name="PerRequestLifestyle" type="Castle.MicroKernel.Lifestyle.PerWebRequestLifestyleModule, Castle.MicroKernel" preCondition="managedHandler" />
        </modules>
        <handlers>
            <remove name="svc-Integrated" />
            <remove name="xoml-Integrated" />
            <add name="*.vbhtml_*" path="*.vbhtml" verb="*" type="System.Web.HttpForbiddenHandler" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*.vbhtm_*" path="*.vbhtm" verb="*" type="System.Web.HttpForbiddenHandler" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*.cshtml_*" path="*.cshtml" verb="*" type="System.Web.HttpForbiddenHandler" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*.cshtm_*" path="*.cshtm" verb="*" type="System.Web.HttpForbiddenHandler" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*.aspq_*" path="*.aspq" verb="*" type="System.Web.HttpForbiddenHandler" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*.xamlx_*" path="*.xamlx" verb="*" type="System.Xaml.Hosting.XamlHttpHandlerFactory, System.Xaml.Hosting, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*.xoml_*" path="*.xoml" verb="*" type="System.ServiceModel.Activation.HttpHandler, System.ServiceModel.Activation, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*.svc_*" path="*.svc" verb="*" type="System.ServiceModel.Activation.HttpHandler, System.ServiceModel.Activation, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*.soap_*" path="*.soap" verb="*" type="System.Runtime.Remoting.Channels.Http.HttpRemotingHandlerFactory, System.Runtime.Remoting, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*.rem_*" path="*.rem" verb="*" type="System.Runtime.Remoting.Channels.Http.HttpRemotingHandlerFactory, System.Runtime.Remoting, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*.asmx_*" path="*.asmx" verb="*" type="System.Web.Script.Services.ScriptHandlerFactory, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="ScriptResource.axd_GET,HEAD" path="ScriptResource.axd" verb="GET,HEAD" type="System.Web.Handlers.ScriptResourceHandler, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="*_AppService.axd_*" path="*_AppService.axd" verb="*" type="System.Web.Script.Services.ScriptHandlerFactory, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" preCondition="integratedMode,runtimeVersionv2.0" />
            <add name="eurl.axd_*" path="eurl.axd" verb="*" type="System.Web.HttpNotFoundHandler" preCondition="integratedMode,runtimeVersionv2.0" />
        </handlers>
        <validation validateIntegratedModeConfiguration="false" />
    </system.webServer>
</configuration>
